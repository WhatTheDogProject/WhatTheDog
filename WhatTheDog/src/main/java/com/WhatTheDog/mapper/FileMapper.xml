<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.WhatTheDog.mapper.FileMapper">
  	<insert id="regFile">
  		INSERT INTO USER_IMG 
  		VALUES ('F'||LPAD(FILE_SEQ.NEXTVAL,7,'0'), #{ofile_name}, #{sfile_name}, SYSDATE)
  	</insert>
  
  
  </mapper>
  